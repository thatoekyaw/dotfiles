Xft.dpi: 96
*dpi: 96

URxvt.background: [90]#FFFFFF
URxvt.foreground: #000000
URxvt.font: xft:Hack:size=12,xft:FontAwesome:size=12
URxvt.scrollBar: false
URxvt.urgentOnBell: True
URxvt.cursorBlink: True
URxvt.internalBorder: 15
URxvt.depth: 32

! Black + DarkGrey
*color0:  #000000
!*color8:  #555753
! DarkRed + Red
*color1:  #FF0000
!*color9:  #ff8d8d
! DarkGreen + Green
*color2:  #00FF00
!*color10: #c8e7a8
! DarkYellow + Yellow
*color3:  #FFFF00
!*color11: #ffc123
! DarkBlue + Blue
*color4:  #0000FF
!*color12: #3465a4
! DarkMagenta + Magenta
*color5:  #FF00FF
!*color13: #f57900
!DarkCyan + Cyan (both not tango)
*color6:  #00FFFF
!*color14: #46a4ff
! LightGrey + White
*color7:  #FFFFFF
!*color15: #ffffff
